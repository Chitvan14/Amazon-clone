gged out \n        dispatch({\n          type:'SET_USER',\n          user: null\n        })\n      }\n    })\n  }, [])\n\n  //connnection ends \n\n  \n  return (\n    <Router>\n      <div className=\"app\">\n        <Switch>\n\n\n\n          <Route path=\"/checkout\">\n            <Header />\n            <Checkout />\n            <Footer/>\n          </Route>\n          \n          <Route path=\"/payment\">\n            <Header />\n            <Elements stripe={promise}>\n            <Payment/>\n            </Elements>\n          </Route>\n\n          <Route path=\"/orders\">\n          <Header />\n            <Orders />\n            <Footer/>\n          </Route>\n\n\n          <Route path=\"/login\">\n            <Login />\n          </Route>\n\n          {/* default  */}\n          <Route path=\"/\">\n            <Header />\n            <Home />\n            <Footer/>\n          </Route>\n\n\n\n\n\n\n        </Switch>\n      </div>\n    </Router>\n\n  );\n}\n\n\nexport default App;\n","/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/reducer.js",[],"/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/StateProvider.js",[],"/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/Home.js",[],"/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/firebase.js",[],"/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/Header.js",["170"],"import React from 'react';\nimport \"./Header.css\";\nimport { Link } from \"react-router-dom\";\nimport SearchIcon from \"@material-ui/icons/Search\";\nimport ShoppingBasketIcon from \"@material-ui/icons/ShoppingBasket\";\nimport { useStateValue } from \"./StateProvider\";\nimport { auth } from \"./firebase\";\n\n\n\nfunction Header() {\n const[{basket,user},dispatch]= useStateValue();\n\n const handleAuthentication =() => {\n     if(user){\n\n        auth.signOut();\n     }\n }\n  \n\n\n    return (\n        <nav className=\"header\">\n            {/* logo on left  */}\n            <Link to=\"/\">\n\n                <img className=\"header__logo\" src=\"http://pngimg.com/uploads/amazon/amazon_PNG11.png\" alt=\"logo\" />\n            </Link>\n\n            {/* search box  */}\n            <div className=\"header__search\">\n                <input type=\"text\" className=\"header__searchInput\" />\n                <SearchIcon className=\"header__searchIcon\" />\n            </div>\n\n           \n            <div className=\"header__nav\">\n             \n                <Link to={!user && '/login'} className=\"header__link\">\n                    <div className=\"header__option\" onClick={handleAuthentication}>\n                        <span className=\"header__optionLineOne\">Hello {!user ? 'Guest' : user.email}</span>\n                        <span className=\"header__optionLineTwo\">{user ? 'Sign Out' : 'Sign In'}</span>\n                    </div>\n                </Link>\n               \n                <Link to=\"/orders\" className=\"header__link\">\n                    <div className=\"header__option\">\n                        <span className=\"header__optionLineOne\">Returns</span>\n                        <span className=\"header__optionLineTwo\">Orders</span>\n                    </div>\n                </Link>\n                {/* link 3  */}\n                <Link to=\"/\" className=\"header__link\">\n                    <div className=\"header__option\">\n                        <span className=\"header__optionLineOne\">Your</span>\n                        <span className=\"header__optionLineTwo\">Prime</span>\n                    </div>\n                </Link>\n                {/* link 4  */}\n                <Link to=\"/checkout\" className=\"header__link\">\n                    <div className=\"header__optionBasket\">\n\n                        {/* basket icon  */}\n                        <ShoppingBasketIcon />\n                        {/* number of item  in basket*/}\n                        <span className=\"header__optionLineTwo header__basketCount\">{basket?.length}</span>\n                    </div>\n                </Link>\n\n            </div>\n\n\n        </nav>\n    )\n}\n\nexport default Header\n","/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/Login.js",["171"],"import React, { useState } from 'react';\nimport './Login.css'\nimport { Link, useHistory } from \"react-router-dom\";\nimport { auth } from \"./firebase\";\n\nfunction Login() {\n    const history = useHistory();\n    const [email, setEmail] = useState('');\n    const [password, setPassword] = useState('');\n\n    const signIn = e => {\n        e.preventDefault();\n\n        auth\n            .signInWithEmailAndPassword(email, password)\n            .then(auth => {\n                 // it successfully created a new user with email and password\n                history.push('/')\n            })\n            // not successfull \n            .catch(error => alert(error.message))\n    }\n\n    const register = e => {\n        e.preventDefault();\n\n        auth\n            .createUserWithEmailAndPassword(email, password)\n            .then((auth) => {\n                // it successfully created a new user with email and password\n                if (auth) {\n                    history.push('/')\n                }\n            })\n            .catch(error => alert(error.message))\n    }\n\n    return (\n        <div className='login'>\n            <Link to='/'>\n                <img\n                    className=\"login__logo\"\n                    src='https://upload.wikimedia.org/wikipedia/commons/thumb/a/a9/Amazon_logo.svg/1024px-Amazon_logo.svg.png' \n                />\n            </Link>\n\n            <div className='login__container'>\n                <h1>Sign-in</h1>\n\n                <form>\n                    <h5>E-mail</h5>\n                    <input type='text' value={email} onChange={e => setEmail(e.target.value)} />\n\n                    <h5>Password</h5>\n                    <input type='password' value={password} onChange={e => setPassword(e.target.value)} />\n\n                    <button type='submit' onClick={signIn} className='login__signInButton'>Sign In</button>\n                </form>\n\n                <p>\n                    By signing-in you agree to the AMAZON FAKE CLONE Conditions of Use & Sale. Please\n                    see our Privacy Notice, our Cookies Notice and our Interest-Based Ads Notice.\n                </p>\n\n                <button onClick={register} className='login__registerButton'>Create your Amazon Account</button>\n            </div>\n        </div>\n    )\n}\n\nexport default Login","/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/Footer.js",[],"/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/Payment.js",["172"],"import React, { useState, useEffect } from 'react';\nimport \"./Payment.css\";\nimport { useStateValue } from \"./StateProvider\";\nimport CheckoutProduct from \"./CheckoutProduct\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport { CardElement, useStripe, useElements } from \"@stripe/react-stripe-js\";\nimport CurrencyFormat from 'react-currency-format';\nimport { getBasketTotal } from './reducer';\nimport axios from \"./axios\";\nimport {db} from \"./firebase\";\n\nfunction Payment() {\n\n    const history = useHistory();\n    const stripe = useStripe();\n    const elements = useElements();\n    const [{ basket, user }, dispatch] = useStateValue();\n    const [error, setError] = useState(null);\n    const [disabled, setDisabled] = useState(true);\n    const [succeeded, setSucceeded] = useState(false);\n    const [processing, setProcessing] = useState(\"\");\n    const [clientSecret, setClientSecret] = useState(true);\n\n    useEffect(() => {\n        // generate teh special stripe secrete which allow us to charge a customer \n        const getClientSecret = async () => {\n            const response = await axios({\n                method: 'post',\n                // stripe expects the total in currency submits \n                url: `/payments/create?total=${getBasketTotal(basket) * 100}`\n            });\n            setClientSecret(response.data.clientSecret)\n        }\n        getClientSecret();\n    }, [basket])\n    console.log('THE SECRET IS >>', clientSecret)\n    const handleSubmit = async (event) => {\n        // do all other stuffs \n        event.preventDefault();\n        setProcessing(true);\n\n        const payload = await stripe.confirmCardPayment(clientSecret, {\n            payment_method: {\n                card: elements.getElement(CardElement)\n            }\n        }).then(({ paymentIntent }) => {\n\n            db.collection('users').doc(user?.uid).collection('orders').doc(paymentIntent.id).set({\n                basket: basket,\n                amount: paymentIntent.amount,\n                created: paymentIntent.created\n            })\n            // payment confirmation \n            setSucceeded(true);\n            setError(null)\n            setProcessing(false)\n\n            dispatch({\n                type:'EMPTY_BASKET'\n            })\n            history.replace('/orders')\n        })\n    }\n    const handleChange = event => {\n        // listen to changes in the card element \n        // also display any error as the customer type thier card details \n        setDisabled(event.empty);\n        setError(event.error ? event.error.message : \"\");\n    }\n\n    return (\n        <div className=\"payment\">\n            <div className=\"payment__container\">\n                <h1>\n                    Checkout(<Link to=\"/checkout\">{basket?.length} items</Link>)\n                </h1>\n                {/* payment address */}\n                <div className=\"payment__section\">\n                    <div className=\"payment__title\">\n                        <h3>Delivery Address</h3>\n                    </div>\n                    <div className=\"payment__address\">\n                        <p>{user?.email}</p>\n                        <p>123 Colony</p>\n                        <p>Delhi,India</p>\n                    </div>\n\n                </div>\n\n\n                {/* review items  */}\n                <div className=\"payment__section\">\n                    <div className=\"payment__title\">\n                        <h3>Review Items and Delvery</h3>\n                    </div>\n                    <div className=\"payment__items\">\n\n                        {basket.map(item =>\n                        (<CheckoutProduct\n                            id={item.id}\n                            title={item.title}\n                            image={item.image}\n                            price={item.price}\n                            rating={item.rating}\n                        />\n                        ))}\n                    </div>\n                </div>\n\n\n                {/* payment method  */}\n                <div className=\"payment__section\">\n                    <div className=\"payment__title\">\n                        <h3>Payment Method</h3>\n                    </div>\n                    <div className=\"payment__details\">\n                        {/* stripe magic */}\n                        <form onSubmit={handleSubmit}>\n                            <CardElement onChange={handleChange} />\n                            <div className=\"payment__priceContainer\">\n                                <CurrencyFormat\n                                    renderText={(value) => (\n                                        <h3>Order Total : {value}</h3>\n\n                                    )}\n                                    decimalScale={2}\n                                    value={getBasketTotal(basket)}\n                                    displayType={\"text\"}\n                                    thousandSeparator={true}\n                                    prefix={\"₹\"}\n                                />\n                                <button disabled={processing || disabled || succeeded}>\n                                    <span>{processing ? <p>Processing</p> : \"Buy Now\"}</span>\n                                </button>\n                            </div>\n                            {/* error  */}\n                            {error && <div>{error}</div>}\n                        </form>\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Payment\n","/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/Orders.js",["173","174"],"import React,{ useState,useEffect } from 'react';\nimport \"./Orders.css\";\nimport { db } from \"./firebase\";\nimport { useStateValue } from \"./StateProvider\";\nimport Order from \"./Order\";\n\nfunction Orders() {\n    const [{ basket,user }, dispatch] = useStateValue();\n    const [orders,setOrders] = useState([]);\n    useEffect(()=>{\n        if(user){\n            db.collection('users').doc(user?.uid).collection('orders').orderBy('created','desc').onSnapshot(snapshot => (\n                setOrders(snapshot.docs.map(doc =>({\n                    id: doc.id,\n                    data: doc.data()\n                })))\n            ))\n        }else{\n            setOrders([])\n        }\n\n    },[user])\n    return (\n        <div className=\"orders\">\n            <h1>Your Orders</h1>\n            <div className=\"orders__order\">\n                {orders?.map(order =>(\n                    <Order order={order}/>\n                ))}\n            </div>\n        </div>\n    )\n}\n\nexport default Orders\n","/Volumes/FCMAC0010/Users/Storage /Programming/websiteDev/amazon-clone/src/Checkout.js",["175"],"import React from 'react';\nimport \"./Checkout.css\";\nimport Subtotal from \"./Subtotal.js\";\nimport { useStateValue } from \"./StateProvider\";\nimport Che